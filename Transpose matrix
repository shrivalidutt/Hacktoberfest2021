#include <stdio.h>

int main() {
  int m, n, i, j;
  printf("Enter the number of rows and columns of the matrix: ");
  scanf("%d %d", &m, &n);

  int matrix[m][n];
  printf("Enter the elements of the matrix: ");
  for (i = 0; i < m; i++) {
    for (j = 0; j < n; j++) {
      scanf("%d", &matrix[i][j]);
    }
  }

  int transpose[n][m];
  for (i = 0; i < n; i++) {
    for (j = 0; j < m; j++) {
      transpose[i][j] = matrix[j][i];
    }
  }

  printf("The transpose of the matrix is: \n");
  for (i = 0; i < n; i++) {
    for (j = 0; j < m; j++) {
      printf("%d ", transpose[i][j]);
    }
    printf("\n");
  }

  return 0;
}
